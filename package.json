{
  "name": "adriansieber",
  "private": true,
  "scripts": {
    "buildResume": "node ./_resume/index.js",
    "buildCss": "stylus --compress --include-css --out ./css styl/screen.styl",
    "watchCss": "stylus --watch --include-css --out ./css styl/screen.styl",
    "serve": "bundle exec jekyll serve",
    "serve-drafts": "bundle exec jekyll serve --trace --drafts",
    "start": "npm run watchCss & npm run serve-drafts",
    "build": "npm run buildCss && npm run buildResume && bundle exec jekyll build",
    "test": "npm run build",
    "prepublish": "npm run build",
    "precommit": "npm run prepublish"
  },
  "dependencies": {
    "handlebars": "^4.0.5",
    "js-yaml": "^3.6.0",
    "stylus": "^0.54.5"
  },
  "devDependencies": {
    "husky": "^0.11.4"
  },
  "description": "My website on Github. Great! =)",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/adius/adius.github.io"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adius/adius.github.io/issues"
  },
  "homepage": "https://github.com/adius/adius.github.io#readme",
  "keywords": [
    "adriansieber"
  ]
}
